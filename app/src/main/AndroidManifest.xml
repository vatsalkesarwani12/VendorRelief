<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.CodeNaroNa.vendor.relief">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/MaterialTheme">

        <activity android:name=".GlobalHelpers.BaseActivity"/>

        <activity
            android:name=".MainActivity.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme1" />

        <activity
            android:name=".VendorActivity.VendorActivity"
            android:screenOrientation="portrait"
            android:theme="@style/MaterialTheme" />

        <activity
            android:name=".SignUpActivity.SignUp"
            android:screenOrientation="portrait"
            android:theme="@style/MaterialTheme" />
        <activity
            android:name=".SplashActiviy.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>